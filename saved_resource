$(function(){
  $('.menu-link').prop('status','closed');
  $('.menu-link').click(function(){
  	if($('.menu-link').prop('status')=='closed'){
	  	$('.menu-link .menu-line, .menu').addClass('open');
     	$('.menu-link').prop('status','open');
    } else {
    	$('.menu-link .menu-line, .menu').removeClass('open');
     	$('.menu-link').prop('status','closed');
    }
  });
  $('.menu-button').click(function(){
  	$('.menu-link .menu-line, .menu').removeClass('open');
    $('.menu-link').prop('status','closed');
  });
});
  
